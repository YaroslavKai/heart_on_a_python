import turtle
screen = turtle.Screen()
screen.bgcolor("white")
t = turtle.Turtle()
t.pensize(3)
t.speed(10)
t.penup()
t.goto(0,-200)
t.pendown()
t.color("red")
t.fillcolor("red")
t.begin_fill()
t.left(45)
t.forward(150)
t.circle(60,180)
t.right(90)
t.circle(60,180)
t.forward(150)
t.end_fill()
t.hideturtle()
turtle.done()
